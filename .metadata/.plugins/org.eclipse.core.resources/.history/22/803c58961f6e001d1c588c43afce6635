package com.virtusa;

import java.io.FileInputStream;

public class FileInput {

	public static void main(String[] args) {
		try {
			FileInputStream fis = new FileInputStream("D:/demo1.txt");
			int ch;
			while((ch = fis.read())==-1)
			{
				System.out.println((char)ch);
				ch++;
			}
		} catch (Exception e) {
			// TODO: handle exception
		}

	}

}
